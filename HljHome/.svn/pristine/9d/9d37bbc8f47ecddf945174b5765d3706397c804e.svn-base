<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/launcher52_bg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="35.0dip"
        android:paddingRight="35.0dip"
        android:paddingTop="10.0dip" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textColor="#ffb2b2b2"
            android:textSize="36.0dip" />

        <ImageView
            android:layout_width="2.0dip"
            android:layout_height="50.0dip"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:background="#ffb2b2b2" />

        <TextView
            android:id="@id/set_name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:textColor="#ff404040"
            android:textSize="18.0dip" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="50.0dip" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50.0dip"
            android:background="@drawable/setpop"
            android:orientation="vertical" >

            <TextView
                android:id="@id/set_name2"
                android:layout_width="wrap_content"
                android:layout_height="50.0dip"
                android:layout_marginLeft="150.0dip"
                android:gravity="bottom"
                android:textColor="@color/white"
                android:textSize="36.0dip" />

            <ViewStub
                android:id="@id/set_net_set"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:layout_marginTop="15.0dip"
                android:layout="@layout/setting_net" />

            <ViewStub
                android:id="@id/set_server_set"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:layout="@layout/setting_server" />

            <ViewStub
                android:id="@id/set_weather"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:layout_marginTop="15.0dip"
                android:layout="@layout/setting_weather" />

            <ViewStub
                android:id="@id/set_channel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:layout="@layout/setting_other" />

            <ViewStub
                android:id="@id/set_grade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:layout="@layout/setting_grade" />

            <ViewStub
                android:id="@id/set_academic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:layout="@layout/setting_academic" />

            <ViewStub
                android:id="@id/set_play"
                android:layout_width="wrap_content"
                android:layout_height="400.0dip"
                android:layout_marginLeft="40.0dip"
                android:layout="@layout/setting_play" />

            <ViewStub
                android:id="@id/set_login_regest"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout="@layout/setting_login" />

            <ViewStub
                android:id="@id/set_regest"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout="@layout/setting_regest" />

            <ViewStub
                android:id="@id/set_clearcache"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout="@layout/setting_clearcache" />

            <ViewStub
                android:id="@id/set_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25.0dip"
                android:layout_marginTop="15.0dip"
                android:layout="@layout/setting_speed" />

            <ViewStub
                android:id="@id/set_display"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="35.0dip"
                android:layout="@layout/setting_display" />

            <ViewStub
                android:id="@id/set_develop"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout="@layout/in_developing" />
        </LinearLayout>

        <ImageView
            android:id="@id/set_item_log"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true" />
    </RelativeLayout>

</LinearLayout>